@model List<string>

@{
	ViewData["Title"] = "Check animal constraints";
}

<div class="display-info">
	@if (Model.Any())
	{
		@foreach (string item in Model)
		{
			<ul>@item</ul>
		}
	}
	else
	{
		<a>Failed to load constraints list.</a>
	}
	<a asp-action="Index">Back to List</a>
	<a asp-action="Details" asp-route-id=@ViewBag.Id>Back to Details</a>
</div>